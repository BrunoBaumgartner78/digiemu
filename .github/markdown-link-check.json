{
  "retryOn429": true,
  "retryCount": 2,
  "fallbackRetryDelay": "2s",
  "aliveStatusCodes": [200, 206, 301, 302, 303, 307, 308, 403],
  "ignorePatterns": [
    { "pattern": "^https://npm\\.im/next-auth" },
    { "pattern": "^https://npm\\.im/@auth/core" },
    { "pattern": "^https://npm\\.im/" },
    { "pattern": "^\\.?/app/etc/local\\.xml\\.template$" },
    { "pattern": "^\\.?/src/app/etc/local\\.xml\\.template$" },
    { "pattern": "^app/etc/local\\.xml\\.template$" },
    { "pattern": "^src/app/etc/local\\.xml\\.template$" },

    { "pattern": "^\\.?/app/etc/" },
    { "pattern": "^\\.?/src/app/etc/" },

    { "pattern": "^node_modules/" }
  ]
}
